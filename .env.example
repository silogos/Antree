# ============================================================================
# REQUIRED: Copy this file to .env and configure your values
# The application will not start without DATABASE_URL set
# ============================================================================

# Environment Variables for Backend

# Database Configuration
POSTGRES_USER=antree_user
POSTGRES_PASSWORD=antree_password
POSTGRES_DB=antree_db
POSTGRES_PORT=5432

# Database URL (for backend connection) - REQUIRED
DATABASE_URL=postgres://antree_user:antree_password@localhost:5432/antree_db

# Backend Configuration
PORT=3001
NODE_ENV=development
